@font-face{font-family:mathilde;src:url(/fonts/mathilde.otf);font-weight:400;font-display:swap}@font-face{font-family:raleway;src:url(/fonts/Raleway-Regular.ttf);font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:raleway;src:url(/fonts/Raleway-Medium.ttf);font-weight:700;font-style:normal;font-display:swap}*{box-sizing:border-box;margin:0}*:focus{outline:none}body{overflow-x:hidden;height:100vh}ul{list-style-type:none;padding:0}a{text-decoration:none}h1{font-family:mathilde;font-size:10rem;text-align:center;color:#a184b7;font-weight:400}h2{font-family:mathilde;font-size:5rem;line-height:4rem;text-align:left;color:#a184b7;font-weight:400}h3{font-family:raleway;font-weight:700;font-size:1.2em;color:#a184b7}h4{font-family:mathilde;font-size:4em;text-align:center;color:#a184b7;font-weight:400}body,li,a,p,button{font-family:raleway;font-size:1em;color:#6e6e6e;font-weight:400}p{line-height:2.3em;text-align:justify}.tarifs{font-family:raleway;font-size:3.5em;text-align:center;font-weight:400}section{width:80%;margin:auto;margin-bottom:50px}button,.button{padding:.5rem 3rem;border-radius:5rem;border:2px solid #634b7f;background-color:#634b7f;color:#fff;transition:all .3s;cursor:pointer;text-align:center;margin:0 auto}button:hover,.button:hover{border:2px solid #fff;background-color:transparent}button--alt:hover,.button--alt:hover{background-color:transparent;color:#634b7f;border:2px solid #634b7f}.center{text-align:center}.white{color:#fff}.overlay{position:absolute;background:rgba(0,0,0,.6);top:0;right:0;bottom:0;left:0}.rounded{overflow:hidden;border-radius:50%}.topbar{background-color:#634b7f;padding:.2rem 10%;display:flex;height:1.6rem}.topbar-contact{flex:1;display:flex}.topbar-contact a,.topbar-contact li{margin-right:2rem;color:#fff;font-size:.8rem}.topbar-contact img{margin-right:.5rem;vertical-align:bottom}.topbar-social{display:flex;align-items:center}.topbar-social a{height:100%;display:block;margin-left:1em}.topbar-social img{vertical-align:middle}.navbar{position:sticky;top:0;z-index:9999;flex:1;display:flex;align-items:stretch;padding:0 10%;background-color:#fff;height:6rem;box-shadow:2px 2px 18px #634b7fb3;gap:2rem}.navbar a{display:flex;align-items:center;color:#6e6e6e}.navbar a:hover{color:#d0184d}.navbar .active{color:#d0184d}.navbar .spacing{flex:1}#banner{width:100%;position:relative;display:flex;flex-direction:column;align-items:center;justify-content:space-evenly;height:calc(100vh - 7.6rem);overflow:hidden;background-position:50%;background-attachment:fixed;background-repeat:no-repeat;background-size:cover}#banner>img{position:absolute;top:0;z-index:-1}#banner>h1{color:#fff;text-shadow:2px 2px 12px #000}.splitted-section{display:grid;grid-template-columns:1fr 1fr}.splitted-section--alt{margin:0;padding:4rem 10%;width:100%;background-color:#a184b7;position:relative}.splitted-section--alt .splitted-section__text{padding:3rem}.splitted-section--alt::after{content:url(/img/flower.svg);position:absolute;bottom:0;right:7%;width:12%}.splitted-section--inverted .splitted-section__image{order:1}.splitted-section--inverted .splitted-section__text{order:0}.splitted-section__image{position:relative;padding-top:100%;max-height:0}.splitted-section__image img{position:absolute;top:0;left:0;right:0;bottom:0;width:100%;height:100%;display:block;object-fit:cover}.splitted-section__text{display:flex;flex-direction:column;padding:3rem 0 3rem 3rem;gap:2rem;justify-content:center;align-items:flex-start;background-color:#fff}.splitted-section__text .blockquote{position:relative;padding:1rem 0}.splitted-section__text .blockquote p{color:#634b7f;line-height:1.5rem;font-weight:700;padding:0 3rem;text-align:center}.splitted-section__text .blockquote img{position:absolute;width:2.5rem;height:2.5rem}.splitted-section__text .blockquote img:first-of-type{top:0}.splitted-section__text .blockquote img:last-of-type{bottom:0;right:0;transform:rotate(180deg)}@media(max-width:1150px){.header__topbar{display:none}}